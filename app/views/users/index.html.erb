<div class="container">
  <div class="row">
      <%= render 'users/lists', user:@user, book:@book, book_new:@book_new %>
  <div class="col-xs-9">
   <h2>Users</h2>
    <table class="table">
     <thead>
      <tr>
       <th>image</th>
       <th>name</th>
       <th></th>
      </tr>
    </thead>
      <% @users.each do |user| %>
       <tr>
         <td>
        <%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg", class: "img-circle pull-left profile-thumb" %>
        </td>
         <td>
         <%= user.name %>
         </td>
         <td>
         <%= link_to 'Show', user_path(user) %>
         </td>
       </tr>
     <% end %>
   </table>
  </div>
 </div>
</div>